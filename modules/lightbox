import GLightbox from 'glightbox';
import { headerContactFormContainer } from './headercontactform';

export const lightbox = GLightbox({
	openEffect: 'fade',
	closeEffect: 'fade',
	selector: '.glightbox',
	draggable: false,
	width: '500px',
	height: 'auto',
	zoomable: 'false',
	closeButton: 'false',
});

lightbox.insertSlide(
	{
		content: headerContactFormContainer,
	},
	1
);
